    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.appmestrado.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.appmestrado.ui.gallery.GalleryFragment"
        android:label="Eletricidade"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.appmestrado.ui.slideshow.SlideshowFragment"
        android:label="Localização"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_bluetooth"
        android:name="com.example.appmestrado.ui.BluetoothFragment"
